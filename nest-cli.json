{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/travel-package-proposal-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true,
    "tsConfigPath": "apps/travel-package-proposal-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/travel-package-proposal-api",
  "projects": {
    "travel-package-proposal-api": {
      "type": "application",
      "root": "apps/travel-package-proposal-api",
      "entryFile": "main",
      "sourceRoot": "apps/travel-package-proposal-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/travel-package-proposal-api/tsconfig.app.json"
      }
    },
    "iam": {
      "type": "application",
      "root": "apps/iam",
      "entryFile": "main",
      "sourceRoot": "apps/iam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/iam/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}