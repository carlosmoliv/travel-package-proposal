{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/iam/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true,
    "tsConfigPath": "apps/iam/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/iam",
  "projects": {
    "iam": {
      "type": "application",
      "root": "apps/iam",
      "entryFile": "main",
      "sourceRoot": "apps/iam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/iam/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "travel-package": {
      "type": "application",
      "root": "apps/travel-package",
      "entryFile": "main",
      "sourceRoot": "apps/travel-package/src",
      "compilerOptions": {
        "tsConfigPath": "apps/travel-package/tsconfig.app.json"
      }
    },
    "proposal": {
      "type": "application",
      "root": "apps/proposal",
      "entryFile": "main",
      "sourceRoot": "apps/proposal/src",
      "compilerOptions": {
        "tsConfigPath": "apps/proposal/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    }
  }
}